<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/common_base" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_13"
            android:text="可提现金额"
            android:textColor="@color/color_00"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="@dimen/dp_15"
            android:background="@null"
            android:gravity="center_vertical"
            android:hint=""
            android:textSize="@dimen/sp_10"
            tools:ignore="RtlCompat" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"

        android:background="@color/white">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_13"
            android:text="申请提现金额"
            android:textColor="@color/color_00"
            android:textSize="@dimen/sp_12" />

        <EditText
            android:id="@+id/pay_Account"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="@dimen/dp_15"
            android:background="@null"
            android:hint="输入提现金额"
            android:text=""
            android:gravity="center_vertical|end"
            android:textSize="@dimen/sp_10"
            tools:ignore="RtlCompat" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_20"
        android:background="@color/white"
        android:orientation="vertical">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_15"
            android:text="提现到"
            android:textColor="@color/color_00"
            android:textSize="@dimen/sp_14" />


        <RelativeLayout
            android:layout_width="@dimen/dp_143"
            android:layout_height="@dimen/dp_44"
            android:layout_alignParentEnd="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_21"
            android:background="@mipmap/pressedborder"
            tools:ignore="RtlCompat">

            <ImageView

                android:layout_width="@dimen/dp_21"
                android:layout_height="@dimen/dp_18"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/dp_23"
                android:layout_marginLeft="@dimen/dp_23"
                android:layout_marginBottom="@dimen/dp_13"
                android:src="@mipmap/bank" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="@dimen/dp_22"
                android:layout_marginRight="@dimen/dp_22"
                android:layout_marginBottom="@dimen/dp_11"
                android:text="银行账户"
                android:textColor="@color/color_00"
                android:textSize="@dimen/sp_17" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="43.5dp"
            android:layout_marginLeft="@dimen/dp_29"
            android:layout_marginTop="@dimen/dp_50"
            android:layout_marginRight="@dimen/dp_15">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="卡号"
                android:textColor="@color/color_00"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/Cardnumber"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_83"
                android:layout_marginLeft="@dimen/dp_83"
                android:background="@null"
                android:gravity="center_vertical"
                android:hint=""
                android:textColorHint="@color/color_00"
                android:textSize="@dimen/sp_13" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:background="#CACACA" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="43.5dp"

            android:layout_marginLeft="@dimen/dp_29"
            android:layout_marginRight="@dimen/dp_15">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="开户行"
                android:textColor="@color/color_00"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/Openingbank"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_83"
                android:layout_marginLeft="@dimen/dp_83"
                android:background="@null"
                android:gravity="center_vertical"
                android:hint=""
                android:textColorHint="@color/color_00"
                android:textSize="@dimen/sp_13" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:background="#CACACA" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="43.5dp"

            android:layout_marginLeft="@dimen/dp_29"
            android:layout_marginRight="@dimen/dp_15">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="具体银行"
                android:textColor="@color/color_00"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/Specificbanks"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_83"
                android:layout_marginLeft="@dimen/dp_83"
                android:background="@null"
                android:gravity="center_vertical"
                android:hint=""
                android:textColorHint="@color/color_00"
                android:textSize="@dimen/sp_13" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:background="#CACACA" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="43.5dp"

            android:layout_marginLeft="@dimen/dp_29"
            android:layout_marginRight="@dimen/dp_15">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="户名"
                android:textColor="@color/color_00"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/Accountname"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="@dimen/dp_83"
                android:layout_marginLeft="@dimen/dp_83"
                android:background="@null"
                android:gravity="center_vertical"
                android:hint=""
                android:textColorHint="@color/color_00"
                android:textSize="@dimen/sp_13" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/edit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/dp_15"
                android:text="编辑"
                android:textColor="#999999"
                android:textSize="@dimen/sp_12"
                tools:ignore="RtlCompat" />
        </RelativeLayout>

        <TextView
            android:id="@+id/Bind_immediately"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_35"
            android:layout_marginRight="@dimen/dp_20"
            android:background="@drawable/bg_8dborder"
            android:gravity="center"
            android:text="提交申请"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_17" />
    </LinearLayout>


</LinearLayout>
